{
  "name": "astro-paper",
  "version": "2.1.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "format": "prettier --plugin-search-dir=. --write .",
    "prepare": "husky install",
    "lint": "eslint .",
    "post": "node ./scripts/create-posts.js"
  },
  "dependencies": {
    "-": "latest",
    "@astrojs/rss": "latest",
    "astro": "latest",
    "fuse.js": "latest",
    "github-slugger": "latest",
    "minimist": "latest",
    "prettier-plugin-astro": "latest",
    "remark-collapse": "latest",
    "remark-toc": "latest",
    "remark-validate-links": "latest",
    "sanitize-filename": "latest",
    "satori": "latest",
    "sharp": "latest",
    "tailwindcss": "latest",
    "unified": "latest",
    "unist-util-visit": "latest",
    "vite": "latest"
  },
  "devDependencies": {
    "@astrojs/react": "latest",
    "@astrojs/sitemap": "latest",
    "@astrojs/tailwind": "latest",
    "@tailwindcss/typography": "latest",
    "@types/github-slugger": "latest",
    "@types/react": "latest",
    "@typescript-eslint/parser": "latest",
    "astro-eslint-parser": "latest",
    "commitizen": "latest",
    "cz-conventional-changelog": "latest",
    "eslint": "latest",
    "eslint-plugin-astro": "latest",
    "gray-matter": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "react": "latest",
    "react-dom": "latest",
    "typescript": "latest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --plugin-search-dir=. --write"
    ]
  }
}
